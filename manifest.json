{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "icons": {
    "16": "icons/my-icon/icon-size-16.png",
    "48": "icons/my-icon/icon-size-48.png",
    "128": "icons/my-icon/icon-size-128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/my-icon/icon-size-16.png",
      "48": "icons/my-icon/icon-size-48.png",
      "128": "icons/my-icon/icon-size-128.png"
    }
  },
  "permissions": [
    "activeTab"
  ]
}
